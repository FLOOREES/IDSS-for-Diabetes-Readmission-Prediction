    "# Prep checking\n",
    "import pandas as pd\n",
    "\n",
    "df_raw = pd.read_csv('data/diabetic_data.csv')\n",
    "df_prep = pd.read_csv('data/diabetic_data_preprocessed.csv')\n",
    "\n",
    "race_cols = [col for col in df_prep.columns if col.startswith('race')]\n",
    "df_prep.loc[df_raw.race == '?', race_cols]"
   ]
